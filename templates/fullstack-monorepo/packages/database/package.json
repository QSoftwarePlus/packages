{
  "name": "database",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "browser": "./dist/index-browser.js",
  "types": "index.ts",
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  },
  "scripts": {
    "build": "npm run db:generate && npm run compile",
    "kysely:compile": "tsup ./kysely/types.ts ./dist/types.js",
    "compile": "tsup index.ts index-browser.ts && npm run kysely:compile",
    "db:seed": "dotenv -e .env -- prisma db seed -- --environment production",
    "db:seed:testing": "dotenv -e .env.testing -- npx prisma db seed -- --environment testing",
    "db:seed-dev": "npx prisma db seed -- --environment development",
    "db:generate": "npx prisma generate",
    "db:push:testing": "dotenv -e .env.testing -- prisma db push --skip-generate",
    "db:generate:test": "dotenv -e .env.testing -- prisma generate",
    "db:push": "prisma db push --skip-generate",
    "dev": "npm run compile & prisma generate --watch"
  },
  "dependencies": {
    "@prisma/client": "latest",
    "bcrypt": "^5.1.0",
    "csv": "^6.3.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "kysely": "^0.26.3",
    "mysql2": "^3.5.2",
    "prisma-cursorstream": "^0.3.0",
    "prisma-json-types-generator": "^3.0.0-beta.4",
    "prisma-kysely": "^1.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^20.4.5",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1",
    "tsup": "6.6.2",
    "typescript": "^5.2.2"
  }
}
